    </classes>
  </test>  
  
   <test name="fireFoxTest">
  <parameter name="os" value="Windows"></parameter>
  <parameter name="browser" value="fireFox"></parameter>
    <classes>
      <class name="tetsCases.TC001_ValidateRegistration"/>
    </classes>
  </test> 
  
</suite> <!-- Suite -->


